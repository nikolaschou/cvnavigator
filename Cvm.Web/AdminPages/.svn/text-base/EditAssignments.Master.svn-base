<%@ Register TagPrefix="my" TagName="EditAssignmentsAuxCtrl" Src="~/AdminPages/CommonCtrl/EditAssignmentsAuxCtrl.ascx" %>
<asp:Content ID="Content1" ContentPlaceHolderID="LeftPane" runat="server">
