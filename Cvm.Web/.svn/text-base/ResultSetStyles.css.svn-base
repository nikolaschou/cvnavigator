
/* table classes (datagrid) */
.datagrid
{
	border: 1px solid #000000;
}

.headerStyle
{
	background-color: #cccccc;
	color: Black;
	font-size: 12px;
	font-weight: bold;
	padding-left: 5px;
	height: 22px;
	border-bottom: 1px solid #000000;
	text-align: left;
	white-space:nowrap;
}

thead.headerStyle td
{
	background-color: #cccccc;
	color: Black;
	font-size: 12px;
	font-weight: bold;
	padding-left: 5px;
	height: 22px;
	border-bottom: 1px solid #000000;
	text-align: left;
	white-space:nowrap;
}

.itemtdStyle
{
	border-bottom: 1px solid #CCCCCC;
	
}

.resultset2
{
	background-color: Rgb(239,243,234);
	color: #000000;
	font-size: 11px;
	height: 20px;
	padding-left: 5px;
}

.resultset1
{
	background-color: #ffffff;
	color: #000000;
	font-size: 11px;
	height: 20px;
	padding-left: 5px;
	
}

a.listlink
{
	color: black;
	font-weight:bolder;
	font-size: 11px;
}



.droppedBorder
{
	border-left: 1px solid #808080;
	border-bottom: 1px solid #ffffff;
	border-right: 1px solid #ffffff;
}

.resultsetsearch
{
	background-color: #ffdddd;
	color: #000099;
	font-size: 11px;
	height: 20px;
	padding-left: 5px;
	border-bottom: 1px solid #DDDDDD;
}

.textbox
{
	background-color: #ffffff;
}


.itemstyle0
{
	background-color: Rgb(239,243,234);
	color: black;
	font-size: 11px;
	height: 20px;
	border-bottom:solid 1px Rgb(155,187,89);
}

.itemstyle0 td, .itemstyle1 td 
{
	padding-left:5px;	
}

.itemstyle1
{
	background-color: #ffffff;
	color: black;
	font-size: 11px;
	height: 20px;
	padding-left: 5px;
	margin-left:5px;
	border-bottom:solid 1px Rgb(155,187,89);
	
}

.noaccessTable
{
	margin-top: 4px;
	border-top: 1px solid #333333;
	border-right: 1px solid #ffffff;
	border-bottom: 1px solid #ffffff;
	border-left: 1px solid #333333;
	background-color: #dddddd;
	width: 400px;
	margin-left:20px;
}

.noaccessTD
{
	font-size: 11px;
	color: #990000;
	vertical-align: top;
}

.noaccessDiv
{
	margin-top:20px;
	margin-bottom:20px;
	margin-left:20px;
}

.noaccessHdr
{
	margin-top:20px;
	font-size:12px;
	font-weight:bold;
	color:#666666;
	margin-left:20px;
}


