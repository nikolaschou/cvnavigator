/******** Styling autoforms **********/
div.autoformDiv,div.autoformOuterDiv
{
    border:1px solid gray;
    border-radius:6px;
    -moz-border-radius:6px;
    -webkit-border-radius:6px;
    background-color:#eee;
    padding:15px;
}

/* If an autoformOuterDiv is wrapped around an autoformDiv, the inner div will all styling reset */
div.autoformOuterDiv  div.autoformDiv
{
    border:0;
    border-radius:0;
    -moz-border-radius:0;
    -webkit-border-radius:0;
    background-color:inherit;
    padding:0;
}
