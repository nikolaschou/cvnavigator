/********** Styling selector matrix ****************/
table.matrix 
{
    border:1px solid grey;
}

table.matrix td
{
    padding:1px;
    border:1px solid #333;
    background-color:White;
        width:30px;
    height:20px;
    vertical-align:middle;
}

table.matrix thead td
{
    font-size:8px;
}

table.matrix thead td:first-letter 
{
    font-size:14px;
}

table.matrix td.selector 
{
    background-color:#ccc;
    cursor:pointer;
}

table.matrix td.selector:hover
{
    outline:2px solid black;
}

table.matrix td.marked 
{
    background-color:#0c0;
    cursor:pointer;
}

div#profileMatrixDiv
{
    width:100%;
}
table.matrix td:first-child 
{
    width:180px;
}  

table.matrix img 
{
    cursor:pointer;
}

/********* Autocomplete  ************/
.ui-autocomplete a,.ui-autocomplete a:hover 
{
    font-weight:normal;
}

/******* FIXING ERROR IN Jquery UI date picker ***********/
div#ui-datepicker-div 
{
    display:none;
}



/********* Save and Cancel buttons **************/

input[type=button],  input[type=submit]
{
    cursor:pointer;
    background: rgb(70,105,57); /* for non-css3 browsers */
    border-radius:5px;
    -moz-border-radius:5px;
    -webkit-border-radius:5px;
    color:White;
    font-weight:bolder;
    margin:3px;
    padding:5px 10px 5px 10px;

    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#666', endColorstr='rgb(70,105,57)'); /* for IE */
    background: -webkit-gradient(linear, left top, left bottom, from(rgb(146,200,33)), to(rgb(70,105,57))); /* for webkit browsers */
    background: -moz-linear-gradient(top, #999, #333); /* for firefox 3.6+ */ 
    border:1px solid #666;
}

input[type=button]:hover,  input[type=submit]:hover 
{
           border:1px solid #333;
}

div.buttons 
{
    text-align:right;
    padding-right: 10px;
}
 
input.saveBtn 
{
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='rgb(146,200,33)', endColorstr='rgb(70,105,57)'); /* for IE */
    background: -webkit-gradient(linear, left top, left bottom, from(rgb(146,200,33)), to(rgb(70,105,57))); /* for webkit browsers */
    background: -moz-linear-gradient(top, rgb(146,200,33),  rgb(70,105,57)); /* for firefox 3.6+ */ 
    border:1px solid rgb(183,209,127);
    
}

input.cancelBtn 
{
    background: rgb(141,31,31); /* for non-css3 browsers */
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='rgb(235,58,58)', endColorstr='rgb(141,31,31)'); /* for IE */
    background: -webkit-gradient(linear, left top, left bottom, from(rgb(235,58,58)), to(rgb(141,31,31))); /* for webkit browsers */
    background: -moz-linear-gradient(top, rgb(235,58,58),  rgb(141,31,31)); /* for firefox 3.6+ */ 
    border:1px solid rgb(226,141,141);
    
}

