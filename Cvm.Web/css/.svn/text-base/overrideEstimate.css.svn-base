/* This stylesheet is used to override design og Estimate services.*/
body 
{
    background-color:White;
    background-image:none;
}


input[type=button],  input[type=submit]
{
    cursor:pointer;
    background: rgb(70,105,57); /* for non-css3 browsers */
    -moz-border-radius:5px;
    -webkit-border-radius:5px;
    border-radius:5px;
    color:White;
    font-weight:bolder;
    margin:3px;
    padding:5px 10px 5px 10px;

    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#666', endColorstr='rgb(70,105,57)'); /* for IE */
    background: -webkit-gradient(linear, left top, left bottom, from(rgb(146,200,33)), to(rgb(70,105,57))); /* for webkit browsers */
    background: -moz-linear-gradient(top, #999, #333); /* for firefox 3.6+ */ 
    border:1px solid #666;
}
input[type=button]:hover,  input[type=submit]:hover 
{
           border:1px solid #333;
}
input[type=button], input[type=submit] 
{
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='rgb(146,200,33)', endColorstr='rgb(70,105,57)'); /* for IE */
    background: -webkit-gradient(linear, left top, left bottom, from(rgb(146,200,33)), to(rgb(70,105,57))); /* for webkit browsers */
    background: -moz-linear-gradient(top, rgb(146,200,33),  rgb(70,105,57)); /* for firefox 3.6+ */ 
    border:1px solid rgb(183,209,127);
    
}
